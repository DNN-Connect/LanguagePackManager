{
  "name": "languagepackmanager",
  "version": "1.0.0",
  "description": "DNN Language Pack Manager",
  "dnn": {
    "projectFolders": [
      "Server/LanguagePackManager"
    ],
    "owner": {
      "name": "Peter Donker",
      "organization": "DNN Connect",
      "url": "http://dnn-connect.org",
      "email": "peter@bring2mind.net"
    },
    "pathsAndFiles": {
      "solutionFile": "./Connect.LanguagePackManager.sln",
      "pathToAssemblies": "./bin",
      "devSiteUrl": "",
      "devSitePath": "",
      "excludeFilter": [
        "bin/",
        "obj/",
        "node_modules"
      ],
      "licenseFile": "./License.md",
      "releaseNotesFile": "",
      "releaseFiles": [
        "App_LocalResources/*.resx",
        "**/*.ascx",
        "**/*.cshtml",
        "**/*.vbhtml",
        "js/*.js",
        "css/*.css",
        "*.css",
        "**/*.html",
        "**/*.png",
        "**/*.jpg",
        "**/*.jpeg",
        "**/*.gif",
        "**/*.eot",
        "**/*.svg",
        "**/*.ttf",
        "**/*.woff",
        "**/*.woff2",
        "*.txt"
      ],
      "zipName": "Connect.LanguagePackManager"
    }
  },
  "scripts": {
    "build": "set NODE_ENV=production&&webpack --config Client/webpack.config.js",
    "watch": "webpack --config Client/webpack.config.js --progress --watch",
    "watch-server": "node watch.js"
  },
  "author": "Peter Donker",
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jquery": "^3.5.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "babel-loader": "^8.2.2",
    "chokidar": "^3.5.1",
    "css-loader": "^5.2.4",
    "deepmerge": "^4.2.2",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^5.0.0",
    "fs-extra": "^10.0.0",
    "jquery": "^3.6.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "postcss-loader": "^5.2.0",
    "sass-loader": "^11.1.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.1.2",
    "typescript": "^4.3.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
